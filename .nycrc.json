{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": ["text", "lcov", "html"],
  "include": ["src/**/*.ts"],
  "exclude": ["**/*.test.ts", "test/**", "dist/**", "node_modules/**", "coverage/**"],
  "branches": 80,
  "lines": 85,
  "functions": 80,
  "statements": 85
}
